{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "moduleResolution": "node",
      "outDir": "./build",
      "strict": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "strictPropertyInitialization": false,
      "esModuleInterop": true
   },
   "include": ["src/**/*.ts"],
   "exclude": ["./build"]
} 